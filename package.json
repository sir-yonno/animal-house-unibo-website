{
  "name": "gamearea",
  "version": "0.0.0",
  "scripts": {
    "init": "npm run env-db && npm i --force && npm run init-server && npm run init-db",
    "init-server": "cd server; npm i",
    "client": "vite --host",
    "build": "vite build",
    "build-dev": "vite build && rm -rf server/static/dist && cp -r dist server/static/. && node server/app.js",
    "server": "nodemon server/app.js",
    "init-db": "node server/init_db.js",
    "env-server": "$TERM npm run server &",
    "env-client": "$TERM npm run client &",
    "env-db": "bash -c 'if [[ \"bash -c (cat /etc/os-release| head -n 1)\" == *\"Arch Linux\"* ]]; then bash -c \"sudo systemctl start mongod\"; else bash -c \"sudo systemctl start mongodb\"; fi'",
    "env": "npm run env-db && npm run env-server && npm run env-client ",
    "preview": "vite preview --port 5050"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@vitejs/plugin-react": "^1.3.1",
    "balm-ui": "^6.6.5",
    "bcrypt": "^5.0.1",
    "blueimp-load-image": "^5.16.0",
    "bootstrap": "^5.1.3",
    "bootstrap-vue-3": "^0.1.11",
    "canvas-confetti": "^1.5.1",
    "express": "^4.18.0",
    "express-async-handler": "^1.2.0",
    "gitart-scroll-carousel": "^1.2.4",
    "gitart-vue-dialog": "^2.4.0",
    "lodash": "^4.17.21",
    "lodash.sample": "^4.2.1",
    "mdb-vue-ui-kit": "^3.0.0",
    "node-uuid": "^1.4.8",
    "nodemon": "^2.0.20",
    "phosphor-vue": "^4.4.0",
    "pinia": "^2.0.13",
    "react": "^18.0.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.36.1",
    "react-icons": "^4.4.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "sass": "^1.51.0",
    "styled-components": "^5.3.5",
    "swiper": "^8.3.2",
    "vite-plugin-vue2": "^2.0.2",
    "vue": "^3.2.27",
    "vue-carousel": "^0.18.0",
    "vue-head": "^2.2.0",
    "vue-router": "^4.0.13",
    "vue-slick-carousel": "^1.0.6",
    "vue3-carousel": "^0.1.38",
    "vueperslides": "^3.3.2",
    "wordlist-js": "^1.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "vite": "^2.9.5"
  }
}
